{
  "$schema": "https://raw.githubusercontent.com/WalterWoshid/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "walterwoshid.neutralino.svelte.ts.tailwindcss.template",
  "version": "0.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/bundle",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "Neutralino Svelte TypeScript TailwindCSS Template",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/src/img/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {},
    "cloud": {}
  },
  "cli": {
    "binaryName": "neutralino-js",
    "resourcesPath": "/bundle/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/src/libs/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/src/index.html",
      "devUrl": "http://localhost:8080"
    }
  }
}
