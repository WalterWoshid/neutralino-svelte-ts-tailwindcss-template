{
  "name": "neutralino-svelte-ts-tailwindcss-template",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "neu update",
    "dev": "concurrently -k -n \"Vite,Neutralino\" -c \"#646CFF.bold,#F89901.bold\" \"yon run dev-vite\" \"yon run wait-on tcp:8080 && yon run dev-neu\"",
    "dev-vite": "vite",
    "dev-neu": "neu run --frontend-lib-dev -- --window-enable-inspector",
    "build-vite": "vite build",
    "build-neu": "neu build",
    "build-neu-release": "neu build --release",
    "build": "yon run check && yon run build-vite && yon run build-neu",
    "build-release": "yon run check && yon run build-vite && yon run build-neu-release",
    "check": "tsc && svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "svelte",
    "vite",
    "neutralinojs",
    "typescript",
    "tailwindcss",
    "hmr"
  ],
  "devDependencies": {
    "@neutralinojs/neu": "^9.3.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.9",
    "@tsconfig/svelte": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.18",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.55.0",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^3.1.8",
    "tsc": "^2.0.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^3.1.8",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^3.5.1",
    "wait-on": "^6.0.1",
    "yarn-or-npm": "^3.0.1"
  }
}
